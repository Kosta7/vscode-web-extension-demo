{
  "name": "demo",
  "displayName": "Demo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "browser": "./dist/web/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "demo.authorizeAndFetch",
        "title": "Authorize & Fetch"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "demo.explorer",
          "name": "Github Repos"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "demo.explorer",
        "contents": "[Authorize & Fetch](command:demo.authorizeAndFetch)"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "pnpm run compile-web",
    "vscode:prepublish": "pnpm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package": "vsce package",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
    "dev": "concurrently \"pnpm run watch-web\" \"pnpm run run-in-browser\""
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^10.0.1",
    "@types/vscode": "^1.82.0",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-web": "^0.0.45",
    "@vscode/vsce": "^2.21.0",
    "assert": "^2.0.0",
    "concurrently": "^8.2.1",
    "mocha": "^10.2.0",
    "process": "^0.11.10",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
